<div class="wish-lists container">
  <h1>Create a Wish List</h1>
  
  <form method="POST" action="{{urls.create}}">
    <div class="form-group">
      <label for="wishList_name">Wish List Name</label>
      <input type="text" class="form-control" id="wishList_name" name="wishList[name]">
    </div>
    <button type="submit" class="btn btn-primary">Create</button>
  </form>
</div>